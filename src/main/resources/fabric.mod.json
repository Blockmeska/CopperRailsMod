{
	"schemaVersion": 1,
	"id": "copper-rails",
	"version": "${version}",
	"name": "Copper Rails",
	"description": "Adds copper rails and copper powered rails to Minecraft.",
	"authors": [
		"Blockmeska"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/copper-rails-mod",
		"sources": "https://github.com/Blockmeska/CopperRailsMod"
	},
	"license": "MIT",
	"icon": "assets/copper-rails/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"blockmeska.mod.CopperRails"
		],
		"client": [
			"blockmeska.mod.CopperRailsClient"
		],
		"fabric-datagen": [
			"blockmeska.mod.CopperRailsDataGenerator"
		]
	},
	"mixins": [
		"copper-rails.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15",
		"minecraft": "~1.20",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}